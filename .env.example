# Application
ATLAN_APPLICATION_NAME=postgres
ATLAN_APP_HTTP_HOST=0.0.0.0
ATLAN_APP_HTTP_PORT=3000
ATLAN_TENANT_ID=default

# Temporal
ATLAN_WORKFLOW_HOST=localhost
ATLAN_WORKFLOW_PORT=7233
ATLAN_WORKFLOW_NAMESPACE=default

# Paths
ATLAN_TEMPORARY_PATH=./local/tmp/
ATLAN_SQL_QUERIES_PATH=app/sql

# Dapr component names (defaults match these)
STATE_STORE_NAME=statestore
SECRET_STORE_NAME=secretstore
DEPLOYMENT_SECRET_STORE_NAME=deployment-secret-store
DEPLOYMENT_OBJECT_STORE_NAME=objectstore
UPSTREAM_OBJECT_STORE_NAME=objectstore
EVENT_STORE_NAME=eventstore

# Features
ENABLE_ATLAN_UPLOAD=false

# Optional: Minimum DB server version check (e.g., 12.0)
# ATLAN_SQL_SERVER_MIN_VERSION=

# Example Postgres credentials (not used automatically)
# POSTGRES_HOST=localhost
# POSTGRES_PORT=5432
# POSTGRES_USER=postgres
# POSTGRES_PASSWORD=password
# POSTGRES_DATABASE=postgres

